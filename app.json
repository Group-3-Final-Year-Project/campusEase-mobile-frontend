{
  "expo": {
    "name": "CampusEase",
    "slug": "campusEase",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo1024.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#181820"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo192.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.solo0722.campusEase",
      "versionCode": 6
    },
    "web": {
      "favicon": "./src/assets/images/favicon48.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow CampusEase to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow CampusEase to use your photos."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./src/assets/images/logo96.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "51ca9092-9aad-4259-96b6-3e60cfdf1ee4"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/51ca9092-9aad-4259-96b6-3e60cfdf1ee4"
    }
  }
}
